{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "linebreak-style": "off",
    "arrow-body-style": "off",
    "global-require": "off",
    "no-console": "off",
    "consistent-return": "off",
    "comma-dangle": "off",
    "no-param-reassign": "off"
  }
}